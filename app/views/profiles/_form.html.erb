<%= form_for(@profile),:html=>{:multipart => true }  do |f| %>
  <% if @profile.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@profile.errors.count, "error") %> prohibited this profile from being saved:</h2>
      <ul>
      <% @profile.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :looking_for %><br />
    <%= f.select :looking_for,["friendship","love","meeting new people","adventures"] %>
  </div>
  <div class="field">
    <%= f.label :birthday %><br />
    <%= f.date_select :birthday, :start_year => 1910 %>
  </div>
 
  <div class="field">
    <%= f.label :about %><br />
    <%= f.text_area :about %>
  </div>
    <%= f.file_field :photo%>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
